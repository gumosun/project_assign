<%= form_for @project do |f| %>
  <div class="input-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="input-group">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div class="input-group">
      <%= f.label :Owner %>
      <%= f.select(:user_id) do %>
      <% users.each do |user| %>
      <%= content_tag(:option, user.name, value: user.id) %>
      <% end %>
      <% end %>
  </div>
  <%= f.submit %>
<% end %>